{
  "name": "abris-components",
  "version": "0.1.13",
  "description": "JavaScript components library",
  "homepage": "https://github.com/abris-platform/abris-components",
  "authors": [
    "Serge T <@domain.com>",
    "Fedor D <@domain.com>",
    "Alexey N <@domain.com>"
  ],
  "private": false,
  "license": "MIT",
  "main": "dist/abris-components.js",
  "keywords": [
    "JavaScript",
    "components"
  ],
  "moduleType": "umd",
  "ignore": [
    "index.html",
    "index.debug.html",
    "webpack-pascal-case-name.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abris-platform/abris-components.git"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack",
    "test": "jest",
    "test:dev": "jest --watch",
    "doc:gen": "typedoc sources/index.ts --out docs --readme ./publish/doc-index.md --name '@abris/components' --includeVersion --hideGenerator",
    "release": "standard-version --message \"Release: %s\""
  },
  "dependencies": {
    "jquery": "^3.5.1",
    "knockout": "^3.5.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/jquery": "^3.5.14",
    "@types/knockout": "^3.4.60",
    "@types/node": "^7.0.5",
    "@webpack-cli/serve": "^1.6.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^2.0.0",
    "generate-package-json-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "latest",
    "jest": ">=27.0.0 <28.0.0-0",
    "json-loader": "latest",
    "mini-css-extract-plugin": "latest",
    "sass": "^1",
    "sass-loader": "^8.0.2",
    "standard-version": "^9.3.2",
    "style-loader": "^0.23.1",
    "text-loader": "0.0.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "latest",
    "typedoc": "^0.22.15",
    "typescript": "latest",
    "underscore": "^1.8.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^4.8.1"
  }
}
